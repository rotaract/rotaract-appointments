{
	"name": "rotaract/rotaract-appointments",
	"description": "WordPress plugin for displaying appointments from Elastic.",
	"type": "project",
	"keywords": [
		"wordpress",
		"plugin",
		"rotaract"
	],
	"homepage": "https://github.com/rotaract/rotaract-appointments",
	"readme": "README.adoc",
	"license": "EUPL-1.2",
	"authors": [
		{
			"name": "Rotaract Germany"
		}
	],
	"support": {
		"email": "support@rotaract.de",
		"issues": "https://github.com/rotaract/rotaract-appointments/issues"
	},
	"require": {
		"php": ">=8.0",
		"elasticsearch/elasticsearch": "=v8.10.0",
		"erusev/parsedown": "1.8.0-beta-7"
	},
	"require-dev": {
		"wp-cli/wp-cli-bundle": "^2.8",
		"wp-coding-standards/wpcs": "^3.0",
		"phpcompatibility/php-compatibility": "^9.3"
	},
	"minimum-stability": "stable",
	"scripts": {
		"lint": "phpcs . --standard=phpcs.xml",
		"translate": "wp i18n make-mo languages"
	},
	"config": {
		"platform": {
			"php": "8.0"
		},
		"allow-plugins": {
			"php-http/discovery": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
